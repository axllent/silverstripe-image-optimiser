{
    "name": "axllent/silverstripe-image-optimiser",
    "description": "Automatically optimise all uploaded & resampled images in Silverstripe",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/axllent/silverstripe-image-optimiser",
    "keywords": [
        "silverstripe",
        "upload",
        "image",
        "compress",
        "optimisation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph Slooten",
            "homepage": "http://www.axllent.org/"
        }
    ],
    "support": {
        "issues": "https://github.com/axllent/silverstripe-image-optimiser/issues"
    },
    "require": {
        "silverstripe/framework": "^4.0 || ^5.0",
        "silverstripe/assets": "^1.10 || ^2.0",
        "spatie/image-optimizer": "^1.6.4"
    },
    "suggest": {
        "axllent/silverstripe-scaled-uploads": "Automatically scale down large uploaded images"
    },
    "autoload": {
        "psr-4": {
            "Axllent\\ImageOptimiser\\": "src/"
        }
    }
}
